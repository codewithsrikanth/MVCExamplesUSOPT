@* Specify the type of the data *@
@using MVCDataSharing.Models;
@model List<Student>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>Example with Strongly Typed Views</h1>
        <hr />
        @* Access the Data *@
        <table border="1">
            <tr>
                <th>Student ID</th>
                <th>Student Name</th>
                <th>Gender </th>
            </tr>
            @{ 
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.StudentId</td>
                        <td>@item.Name</td>
                        <td>@item.Gender</td>
                    </tr>
                }
            }
        </table>
    </div>
</body>
</html>
